{
  "global": {
    "help":   ["/usr/bin/printf", "%s", "Commands: /help /echo /uptime /date"],
    "echo":   ["/usr/bin/printf", "%s", "{args}"],
    "uptime": ["/usr/bin/uptime"],
    "date":   ["/usr/bin/date", "-Is=seconds"]
  },

  "alice": {
    "syslog": ["/usr/bin/tail", "-n", "50", "/var/log/syslog"]
  }
}
